node { 
def mnvhome = tool name: 'maven-home', type: 'maven'
    stage('Git-Checkout') { 
        git branch: 'main', credentialsId: 'git-access-token', url: 'https://github.com/EricNk/my-new-program.git'
    }
    stage('Clean-Compile') { 
        sh "${mnvhome}/bin/mvn clean compile" 
    }
    stage('Package') { 
        sh "${mnvhome}/bin/mvn package" 
    }
}

